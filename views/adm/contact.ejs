<!-- Adm Contact List EJS -->

<%- include ("../header") %>
<%- include ("./adm_header") %>


<div class="admconlist_wrap">
    <div class="contactlist_select">
        <form action="/adm/contact" method="post">
            <select name="year">
                <% for(var i = 2017; i < 2100; i++) { %>
                    <% var selected = '' %>
                    <% if(i == nowTime.nowY){ %>
                        <% var selected = 'selected="selected"' %>
                    <% } %>
                    <option value="<%= i %>" <%= selected %>><%= i %></option>
                <% } %>
            </select>
            <select name="month">
                <% for(var i = 1; i < 13; i++) { %>
                    <% var selected = '' %>
                    <% if(i == nowTime.nowM){ %>
                        <% var selected = 'selected="selected"' %>
                    <% } %>
                    <option value="<%= i %>" <%= selected %>><%= i %></option>
                <% } %>
            </select>
            <select name="day">
                <option value="0">전체</option>
                <% for(var i = 1; i < 32; i++) { %>
                    
                    <option value="<%= i %>" <%= selected %>><%= i %></option>
                <% } %>
            </select>
            <input type="text" name="name" value="" placeholder="이름" size="7">
            <input type="text" name="hp" value="" placeholder="연락처" size="8">
            <select name="jusogu">
                <option value="0">구선택</option>
                <option value="원미구">원미구</option>
                <option value="소사구">소사구</option>
                <option value="오정구">오정구</option>
            </select>
        </form>
    </div>
    <table>
        <tr class="admconlist_tit">
            <td>
                번 호
            </td>
